.data
example100:.word 0:100

.macro add_example(%dst)
move $t4,%dst
sll $t4,$t4,2
lw $t3,example100($t4)
add $s0,$s0,$t3
.end_macro

.text
li $t1,0
li $s0,0
for_1_begin:
bge $t1,100,for_1_end
add_example($t1)
addi $t1,$t1,1

for_1_end:
sll $t1,$t1,2
sw $s0,example100($t1)

.data
src:.word 0:100
.align 4
dst:.word 0:100

.macro trans(%dst)
move $t4,%dst
sll $t4,$t4,2
lw $t2,src($t4)
sw $t2,dst($t4)
.end_macro

.text
li $t1,0
for_begin:
bge $t1,100,for_end
trans($t1)
addi $t1,$t1,1

for_end:
li $v0,10
syscall

